{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build_runner: run",
      "type": "dart",
      "command": "dart",
      "args": [
        "run",
        "build_runner",
        "build",
        "--delete-conflicting-outputs",
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "isBackground": true,
    },
    {
      "label": "build_runner: watch",
      "type": "dart",
      "command": "dart",
      "args": [
        "run",
        "build_runner",
        "watch",
        "--delete-conflicting-outputs",
        "--build-filter=\"!lib/openapi/ym_openapi_config.dart\""
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "isBackground": true,
      "runOptions": {
        "runOn": "folderOpen",
        "instanceLimit": 1,
        "reevaluateOnRerun": true
      }
    }
  ]
}
